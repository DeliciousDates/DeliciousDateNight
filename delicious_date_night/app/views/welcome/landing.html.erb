<div class="container">
	<section class="couples">
		<% @couples.each do |couple| %>
		<div class="row">
		  <div class="col-sm-6 col-md-4">
		      <img src= "<%= couple.picture_url %>" alt="..." class="couple-image">
		      <div class="caption">
		        <h4> <%= couple.a_fname %> and <%= couple.b_fname %> </h4>
	      </div>
		<% end %>

		  </div>
		</div>
	</section>
</div>